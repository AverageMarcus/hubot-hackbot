{
  "name": "hubot-hackbot",
  "version": "0.1.3-4",
  "author": "Andrew Seward <abseward@googlemail.com>",
  "contributors": [
    "David Wood <david.p.wood@gmail.com> (http://codesleuth.co.uk/)",
    "Joshua Hawxwell <m@hawx.me> (http://hawx.me/)"
  ],
  "description": "Hackbot is here to help you hack",
  "scripts": {
    "typings": "cd src && typings i",
    "clean": "rimraf build",
    "lint": "tslint -c src/tslint.json -e \"src/**/*.d.ts\" src/**/*.ts",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc -p src",
    "test": "mocha -R dot --compilers coffee:coffee-script/register --require source-map-support/register --recursive build",
    "prepublish": "npm run typings && npm run build && npm test"
  },
  "main": "build/index.js",
  "engines": {
    "node": ">=6.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TechNottingham/hubot-hackbot"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "hubot-slack": "^4.1.0",
    "slug": "^0.9.1",
    "source-map-support": "^0.4.2",
    "yayson": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "express": "^4.14.0",
    "hubot-test-helper": "^1.5.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "scoped-http-client": "^0.11.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "typings": "^1.4.0"
  },
  "keywords": [
    "hubot",
    "hack24",
    "hackbot",
    "technottingham"
  ],
  "bugs": {
    "url": "https://github.com/TechNottingham/hubot-hackbot/issues"
  }
}
